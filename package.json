{
  "name": "fractal-reality",
  "version": "1.0.0",
  "description": "A complete framework bridging physics and metaphysics - deriving quantum mechanics from first principles and validating with LIGO data",
  "main": "index.js",
  "scripts": {
    "test": "node tests/run_four_tests.js",
    "test:watch": "jest --watch",
    "lint": "eslint docs/simulations/*.tsx --ext .ts,.tsx",
    "lint:fix": "eslint docs/simulations/*.tsx --ext .ts,.tsx --fix",
    "format": "prettier --write \"docs/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"docs/**/*.{ts,tsx,js,jsx,json}\"",
    "type-check": "tsc --noEmit",
    "build": "tsc && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.cache",
    "precommit": "npm run format && npm run lint && npm run type-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AshmanRoonz/Fractal_Reality.git"
  },
  "keywords": [
    "physics",
    "quantum-mechanics",
    "fractal",
    "consciousness",
    "gravitational-waves",
    "ligo",
    "metaphysics"
  ],
  "author": "AshmanRoonz",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/AshmanRoonz/Fractal_Reality/issues"
  },
  "homepage": "https://ashmanroonz.github.io/Fractal_Reality/",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.3.0"
  }
}
